package pack06_bool;

public class Ex01_Bool {
     public static void main(String[] args) {
	
    	 //부울형(논리형) : boolean
    	 // true , false 만 기억 > 조건식 작성 시 사용.
    	 // 컴퓨터는 어떤한 판단을 true와 false로만 함.
    	 // 컴퓨터에게 어떠한 질문을 할때는 true, false로 답이 나오는것만 
    	 
    	 	boolean t = true; // 초기화 식
    	 	boolean f = false;
    	 	// 문자열도 표현하고 싶고 변도 표현하고싶다면??
             System.out.println("변수 t에 들어있는 값: + t");
    	 	//변서 f에 들어있는 값? 표현하기
            System.out.println("변수 f에 들어있는 값: f"); 	 	
    	 	
          }
}
