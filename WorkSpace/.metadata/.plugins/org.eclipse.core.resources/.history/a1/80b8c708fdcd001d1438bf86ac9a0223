package pack07_escape;

public class Ex01_Excape {
             public static void main(String[] args) {
				//문자열 상에 어떠한 기호를 넣어서 띄어쓰거나 줄바꿈
            	 System.out.println("이름 : \"김영문 \"");
            	 
            	 System.out.println("변호\t이름"\ t직업 ");
                System.out.println("문자1\n"); //줄바꿈
            	System.out.println("문자2\n");
            	System.out.println("d:\\\\study_java");
            	
}
}