package pack08_escape;

public class Test01_Escape {
  
	public static void main(String[] args) {
		
		System.out.println( "\\\t/\\"       );
		System.out.println( ")\t(  ')"  );
		System.out.println( " (\t/)"       );
		System.out.println( " \\   (__) |"+"");
		
		System.out.println("\t / \\\\/\t");
		System.out.println( "\t"           );

		
	 }
	
	
}
